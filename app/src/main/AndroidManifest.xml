<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jeffrey_zhang.thepuppyapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ThePuppyApp">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MyProfile"
            android:label="@string/title_activity_my_profile" />
        <activity
            android:name=".PuppyProfile"
            android:label="@string/title_activity_puppy_profile" />
        <activity
            android:name=".BrowseDoggies"
            android:label="@string/title_activity_browse_doggies" />
        <activity
            android:name=".BarkBoard"
            android:label="@string/title_activity_bark_board" />
        <activity android:name=".PuppyPlaydates"></activity>
    </application>

</manifest>